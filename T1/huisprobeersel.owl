@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://www.example.org/> .

:Bewoond rdf:type owl:Class ;
	rdfs:label "Bewoond" ;

	rdfs:subClassOf:Woningen ,
		[ rdf:type owl:Restriction ;
		owl:onProperty ex:isHuisinNederland ;
		owl:someValuesFrom ex:HuisinNederland ;
		] ;

		owl:disjointWith ex:HuisinNederland.

:Onbewoond rdf:type owl:Class ;
	rdfs:label "Onbewoond" ;
	
	rdfs:subClassOf:Woningen ,
		[ rdf:type owl:Restriction ;
		owl:onProperty ex:isHuisinNederland ;
		owl:allValuesFrom ex:HuisinNederland ;
		] ;

		owl:disjointWith ex:isHuisinNederland.



ex:NES	rdf:type	ex:Bewoond .
ex:SNES	rdf:type	ex:Bewoond .
ex:N64	rdf:type	ex:Onbewoond .
ex:Gamecube	rdf:type	ex:Bewoond .
ex:Panasonic_Q	rdf:type	ex:Onbewoond .
